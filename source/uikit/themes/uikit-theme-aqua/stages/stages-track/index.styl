.uikit-theme-aqua
    .uikit
        &.uikit-stages
            .uikit-stages-track
                background-color #e5e5e5

            &.horizontal
                .uikit-stages-track
                    width inherit
                    height 5px

            &.vertical
                .uikit-stages-track
                    height inherit
                    width 5px
