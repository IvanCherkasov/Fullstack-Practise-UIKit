include ./radial-progress-caption/index.pug

mixin addUIKitRadialProgress(id, minimum, maximum, def)
	div.uikit.uikit-radial-progress(id=id, data-value=def, data-minimum=minimum, data-maximum=maximum)
		div.wrapper-right
			div.progress-right
		div.wrapper-left
			div.progress-left
		+addUIKitRadialProgress_Caption(def)
