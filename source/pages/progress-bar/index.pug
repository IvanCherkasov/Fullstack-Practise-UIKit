include ../../mixins.pug

doctype html
html(lang='ru')
    head
        meta(charset="utf-8")
        meta(viewport="width=device-width")
        title UIKit - Progress-bar
    body
        div
            a(href='../') Back
        br
        span styles
        select(id="styles-id")
        br
        br
        div(style='width: 100px;')
            +addUIKitProgressBar('uikit-progress-bar-1-h', 25, 'horizontal', true)
        br
        br
        div(style='width: 100px; height: 100px;')
            +addUIKitProgressBar('uikit-progress-bar-2-v', 25, 'vertical', true)
        br
        br
        input(type='submit', value='orientation', id='uikit-submit-id-1', style='width: 100px; height 16px;')
        input(type='text')#input-text-progress
